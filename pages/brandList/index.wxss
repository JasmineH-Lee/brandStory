/* pages/brandList/index.wxss */

.search-box {
  width: 100%;
  height: 30px;
}

.search-box .search-input {
  display: inline-block;
  width: 70%;
  height: 30px;
  line-height: 30px;
  padding: 0 30px 0 30px;
  margin: 0 8px;
  border-radius: 2px;
  color: #999;
  background-color: #f5f5f5;
  font-size: 14px;
}

.search-icon {
  width: 15px;
  height: 15px;
  display: inline-block;
  position: absolute;
  top: 7.5px;
  left: 20px;
}
.delete-icon {
  width: 15px;
  height: 15px;
  display: inline-block;
  position: absolute;
  top: 7.5px;
  right: 52px;
}
.cancelSearch-btn {
  margin-right: 8px;
  display: inline-block;
  height: 30px;
  line-height: 30px;
  color: #999;
  font-size: 14px;
}